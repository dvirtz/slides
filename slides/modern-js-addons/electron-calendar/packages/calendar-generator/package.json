{
  "name": "calendar-generator",
  "private": true,
  "main": "dist/src/ts/interface.js",
  "binary": {
    "napi_versions": [
      7
    ]
  },
  "scripts": {
    "preinstall": "pipenv install && tsc --build",
    "install": "pkg-prebuilds-verify ./dist/src/ts/binding-options.js || npm run build",
    "build": "pipenv run cmake-js build && tsc --build && pkg-prebuilds-verify ./dist/src/ts/binding-options.js",
    "build:debug": "pipenv run cmake-js rebuild -D"
  },
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "pkg-prebuilds": "^0.2.1"
  },
  "devDependencies": {
    "cmake-js": "^7.3.0"
  }
}
